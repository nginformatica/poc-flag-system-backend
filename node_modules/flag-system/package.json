{
    "name": "flag-system",
    "version": "0.9.1",
    "homepage": "https://flags-system.ngi.com.br",
    "displayName": "NGInformatica Flag System",
    "description": "NGInformatica Flag System",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "test": "jest --passWithNoTests --noStackTrace --runInBand",
        "test:verbose": "jest --runInBand",
        "test:staged": "npm test -- --findRelatedTests",
        "test:ci": "npm test -- --coverage",
        "clean:build": "rm -rf ./dist",
        "clean:doc": "rm -rf ./docs",
        "copy": "cp package.json dist && cp README.md dist",
        "build": "yarn clean:build && yarn tsc --emitDeclarationOnly && yarn webpack && yarn copy",
        "build:doc": "yarn clean:doc && yarn typedoc",
        "type-check": "tsc --noEmit",
        "lint": "eslint -c .eslintrc.js 'src/**/*.ts'",
        "pre-commit": "lint-staged",
        "release": "sh ./scripts/release.sh"
    },
    "keywords": [],
    "author": "NGInformatica",
    "license": "ISC",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "7.19.6",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-typescript": "7.18.6",
        "@commitlint/cli": "17.4.3",
        "@commitlint/config-conventional": "17.4.3",
        "@types/jest": "29.4.0",
        "@types/node": "18.13.0",
        "@types/ramda": "0.28.23",
        "@typescript-eslint/eslint-plugin": "5.52.0",
        "babel-cli": "6.26.0",
        "babel-loader": "9.1.2",
        "eslint": "8.34.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-config-standard-with-typescript": "34.0.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-n": "15.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-promise": "6.1.1",
        "husky": "8.0.0",
        "jest": "29.4.2",
        "lint-staged": "13.1.2",
        "prettier": "2.8.4",
        "terser-webpack-plugin": "5.3.6",
        "ts-jest": "29.0.5",
        "ts-loader": "9.4.2",
        "typedoc": "0.23.25",
        "typedoc-plugin-extras": "2.3.2",
        "typedoc-plugin-versions": "0.2.3",
        "typescript": "4.9.5",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1"
    },
    "dependencies": {
        "ramda": "0.28.0"
    }
}
